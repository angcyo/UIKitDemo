<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/base_round_radius_max"/>
            <solid android:color="@color/base_white_press" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/base_round_radius_max"/>
            <solid android:color="@color/base_white_press" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/base_round_radius"/>
            <stroke android:width="@dimen/base_line_px" android:color="@color/wt_line"/>
            <solid android:color="@color/base_white" />
        </shape>
    </item>
</selector>